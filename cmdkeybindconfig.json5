{
	"enabled": true,
	// Keybinding to open the Macro editing screen
	"openMacroScreenKeybind": "key.keyboard.unknown",
	"macros": [
		{
			// Macro keybinding
			"keyName": "key.keyboard.x",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/race upgrade toggle \"origins/arachne/poison_fangs\"",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.period",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/die",
			// Type of Macro. Delayed, Repeating, RunNTimes, SingleUse, DisplayOnly
			"macroType": "SingleUse"
		}
	]
}